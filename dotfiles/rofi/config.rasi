configuration {
  modes: [ combi, drun ];
  combi-modes: [ window, drun ];
  show-icons: true;
  markup: true;
  window-thumbnail: true;
  combi-hide-mode-prefix: true;
  kb-cancel: "Escape,Control+g,Control+bracketleft,Super+space";
  timeout {
      action: "kb-cancel";
      delay:  0;
  }
  filebrowser {
      directories-first: true;
      sorting-method:    "name";
  }
}
@theme "theme"
